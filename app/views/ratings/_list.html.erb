<table class="table table-hover">
  <thead>
  <tr>
    <th><%=title%></th>
    <th>Average</th>
  </tr>
  </thead>

  <tbody>
  <% ratings.keys.each do |b| %>
      <tr>
        <td class="vert-align"><%= link_to b.name, b %></td>
        <td class="vert-align"><%= round(ratings[b]) %></td>
      </tr>
  <% end %>
  </tbody>
</table>